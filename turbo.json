{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": [
    "**/.env.*local"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^db:push",
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".svelte-kit/**",
        ".svelte/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "dependsOn": [
        "^db:generate"
      ]
    },
    "db:push": {
      "cache": false
    },
    "db:generate": {
      "cache": true
    },
    "db:studio": {
      "cache": true
    }
  }
}